// Quadratic: x^2 - 3x - 10 = 0
A = 1;
B = -3;
C = -10;
D = (B * B) - (4 * A * C);
print(D); // discriminant

// Newton iterations to approximate sqrt(D)
E = D / 2;
F = 0;
while (F < 6) {
    E = 0.5 * (E + (D / E));
    F = F + 1;
}

G = (-B + E) / (2 * A);
H = (-B - E) / (2 * A);
print(G);
print(H);
